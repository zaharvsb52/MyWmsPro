<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxreui="http://schemas.devexpress.com/winfx/2008/xaml/richedit" xmlns:dxret="http://schemas.devexpress.com/winfx/2008/xaml/richedit/themekeys">

    <ControlTemplate x:Key="{dxret:RichEditControlThemeKey ResourceKey=PageNumberWindowControlTemplate}" TargetType="{x:Type dxreui:PageNumberWindow}">
        <Border Background="#CF505050" Padding="12,5,12,5" CornerRadius="6" UseLayoutRounding="True" SnapsToDevicePixels="True">
            <TextBlock Text="{TemplateBinding Text}" Foreground="White" FontSize="13" UseLayoutRounding="True" />
        </Border>
    </ControlTemplate>

</ResourceDictionary>
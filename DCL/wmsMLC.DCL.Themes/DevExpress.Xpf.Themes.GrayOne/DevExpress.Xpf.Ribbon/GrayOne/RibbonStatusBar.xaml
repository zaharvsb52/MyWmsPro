<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys">

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=LeftPartTemplate}" TargetType="{x:Type ContentControl}">
        <Border BorderBrush="#FFC4C5CC" BorderThickness="0,1,0,0">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFD8D9DE" Offset="0" />
                    <GradientStop Color="#FFC9CAD1" Offset="1" />
                </LinearGradientBrush>
            </Border.Background>
            <Grid>
                <Border BorderBrush="#3FFFFFFF" BorderThickness="0,1,0,0" />
                <ContentPresenter Margin="1" />
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=RightPartTemplate}" TargetType="{x:Type ContentControl}">
        <Border BorderBrush="#FFC4C5CC" BorderThickness="0,1,0,0">
            <Border.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFD8D9DE" Offset="0" />
                    <GradientStop Color="#FFC9CAD1" Offset="1" />
                </LinearGradientBrush>
            </Border.Background>
            <Grid>
                <Border BorderBrush="#3FFFFFFF" BorderThickness="0,1,0,0" />
                <ContentPresenter Margin="1" />
            </Grid>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=SizeGripTemplate}" TargetType="{x:Type Thumb}">
        <Grid Background="Transparent" Height="10" Width="10" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Path Stretch="Fill" Fill="#7FFFFFFF" Data="F1 M 0,8.99998L 2,8.99998L 2,11L 0,11L 0,8.99998 Z M 3.99997,8.99999L 5.99997,8.99999L 5.99997,11L 3.99997,11L 3.99997,8.99999 Z M 7.99998,8.99999L 9.99998,8.99999L 9.99998,11L 7.99998,11L 7.99998,8.99999 Z M 7.99998,4.99999L 9.99998,4.99999L 9.99998,6.99999L 7.99998,6.99999L 7.99998,4.99999 Z M 7.99999,0.999989L 9.99999,0.999989L 9.99999,2.99998L 7.99999,2.99998L 7.99999,0.999989 Z M 4,4.99998L 6,4.99998L 6,6.99998L 4,6.99998L 4,4.99998 Z " Margin="0,1,0,-1" />
            <Path Stretch="Fill" Fill="#FF868995" Data="F1 M 0,7.99999L 2,7.99999L 2,9.99999L 0,9.99999L 0,7.99999 Z M 3.99997,8L 5.99997,8L 5.99997,10L 3.99997,10L 3.99997,8 Z M 7.99998,8L 9.99998,8L 9.99998,10L 7.99998,10L 7.99998,8 Z M 7.99998,4L 9.99998,4L 9.99998,6L 7.99998,6L 7.99998,4 Z M 7.99999,0L 9.99999,0L 9.99999,2L 7.99999,2L 7.99999,0 Z M 4,4L 6,4L 6,6L 4,6L 4,4 Z " />
        </Grid>
    </ControlTemplate>
    <sys:Double x:Key="{dxrt:RibbonStatusBarThemeKey ResourceKey=MinHeight}">27</sys:Double>
</ResourceDictionary>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ve="http://schemas.devexpress.com/winfx/2008/xaml/docking/visualelements" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/docking/themekeys" xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" xmlns:dxcore_unique_xmlns="http://schemas.devexpress.com/winfx/2008/xaml/core">
  <LinearGradientBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItem}" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="#FFF6F6F9" Offset="0" />
    <GradientStop Color="#FFD2D6DF" Offset="1" />
  </LinearGradientBrush>
  <LinearGradientBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}" EndPoint="0.5,1" StartPoint="0.5,0">
    <GradientStop Color="#FFE4E5EC" Offset="0" />
    <GradientStop Color="#FFD6D9E2" Offset="1" />
  </LinearGradientBrush>
  <SolidColorBrush x:Key="{dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}" Color="#FF7A7999" />
  <ControlTemplate x:Key="{dxt:OtherResourcesThemeKey ResourceKey=HiddenItemTemplate}">
    <Grid Height="Auto" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;16;10">
      <Border x:Name="Item" CornerRadius="2" Opacity="0.5" Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;17;14">
        <Border Margin="1" CornerRadius="1" Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;18;18" />
      </Border>
      <Border dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;20;14">
        <StackPanel VerticalAlignment="Center" Orientation="Horizontal" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;21;18">
          <Image x:Name="PART_Image" Height="16" Width="16" Margin="8,6,0,6" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;22;22" />
          <TextBlock Text="{Binding Path=CustomizationCaption}" Margin="8,6,8,6" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;23;22" />
        </StackPanel>
      </Border>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="{dxt:DockElementsThemeKey ResourceKey=DragCursor}" TargetType="ve:DragCursorControl">
    <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;29;4">
      <Grid x:Name="Item" VerticalAlignment="Top" Opacity="0.75" Visibility="Collapsed" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;30;14">
        <Border Background="#FF000000" Margin="2,2,0,0" CornerRadius="3" Opacity="0.15" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;31;18" />
        <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}" Margin="0,0,2,2" CornerRadius="2" Height="28" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;32;18">
          <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" Margin="1" CornerRadius="1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;33;22" />
        </Border>
      </Grid>
      <Grid x:Name="Panel" Opacity="0.75" Visibility="Collapsed" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;36;5">
        <Border Background="#FF000000" Margin="2,2,0,0" CornerRadius="3" Opacity="0.15" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;37;18" />
        <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemBorder}}" Margin="0,0,2,2" CornerRadius="2" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;38;18">
          <Grid dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;39;7">
            <Grid.RowDefinitions>
              <RowDefinition Height="16" />
              <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItemCaption}}" Margin="1" CornerRadius="1,1,0,0" Grid.Row="0" Opacity="0.75" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;44;26" />
            <Border Background="{DynamicResource {dxt:BrushElementsThemeKey ResourceKey=HiddenItem}}" Margin="1,0,1,1" CornerRadius="0,0,1,1" Grid.Row="1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;45;26" />
          </Grid>
        </Border>
      </Grid>
      <Border x:Name="CaptionContainer" Margin="0,0,2,2" Height="28" VerticalAlignment="Top" HorizontalAlignment="Center" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;49;14">
        <ve:CaptionControl x:Name="PART_CaptionControl" Margin="8,6,8,6" Foreground="#FF201F35" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Docking\GrayOne\CustomizationWindow.xaml;50;18" />
      </Border>
      <VisualStateManager.VisualStateGroups>
        <VisualStateGroup x:Name="CursorTypeStates">
          <VisualState x:Name="CursorTypeItem">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Item" Storyboard.TargetProperty="(Grid.Visibility)" dxcn:ValueSetter.Visibility="Visible" />
            </Storyboard>
          </VisualState>
          <VisualState x:Name="CursorTypePanel">
            <Storyboard>
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Panel" Storyboard.TargetProperty="Visibility" dxcn:ValueSetter.Visibility="Visible" />
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Panel" Storyboard.TargetProperty="Margin" dxcn:ValueSetter.Thickness="0,8,2,2" />
              <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CaptionContainer" Storyboard.TargetProperty="VerticalAlignment" dxcn:ValueSetter.VerticalAlignment="Center" />
            </Storyboard>
          </VisualState>
        </VisualStateGroup>
      </VisualStateManager.VisualStateGroups>
    </Grid>
  </ControlTemplate>

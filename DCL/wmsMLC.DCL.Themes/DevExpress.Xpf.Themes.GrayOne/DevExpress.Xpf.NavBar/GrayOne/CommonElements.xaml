                    <Border Background="#FFE3E9F9" Margin="0,1,0,1" />
                </Border>
                <Border x:Name="Pressed" Background="#FFC8CAD1" BorderBrush="#FFACAEB7" BorderThickness="0,1,0,1" Opacity="0">
                    <Border Background="#FFDADCE2" Margin="0,1,0,0" />
                </Border>
                <Border x:Name="SelectedState" Background="#FFFAFAFB" BorderBrush="#FFACAEB7" BorderThickness="0,1,0,1" Opacity="0" />
            </Grid>
            <dxn:ImageAndTextContentPresenter x:Name="item" ImageFallbackSize="16" Content="{TemplateBinding Content}" ContentTemplate="{Binding Path=Template}" Style="{DynamicResource {dxnt:NavBarItemControlThemeKey ResourceKey=ItemStyle}}" Foreground="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" dx:FocusHelper2.Focusable="False">

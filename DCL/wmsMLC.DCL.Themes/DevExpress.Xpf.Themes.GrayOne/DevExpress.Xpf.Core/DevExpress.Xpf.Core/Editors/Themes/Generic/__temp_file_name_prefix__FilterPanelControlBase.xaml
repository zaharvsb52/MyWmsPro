                <dxef:ComboBoxEdit x:Name="PART_FilterPanelMRUComboBox" VerticalAlignment="Center" IsTextEditable="False" dxcore_unique_xmlns:DXDockPanel.Dock="Left" HorizontalAlignment="Left" ShowEditorButtons="False" ShowBorder="False" EditMode="InplaceActive" ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MRUFilters}" AllowDefaultButton="False" EditNonEditableTemplate="{DynamicResource {dxet:FilterPanelControlBaseThemeKey ResourceKey=MRUComboBoxTemplate}}" DisplayTemplate="{DynamicResource {dxet:FilterPanelControlBaseThemeKey ResourceKey=MRUComboBoxTemplate}}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Editors\Themes\Generic\FilterPanelControlBase.xaml;32;34" />

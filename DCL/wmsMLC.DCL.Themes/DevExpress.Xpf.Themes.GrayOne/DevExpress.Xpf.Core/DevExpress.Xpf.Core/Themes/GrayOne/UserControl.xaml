<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core">

    <ControlTemplate x:Key="{dxt:UserControlThemeKey ThemeName=GrayOne, ResourceKey=ControlTemplate}" TargetType="{x:Type dx:ThemedUserControl}">
        <Border x:Name="PART_Border" Background="Transparent">
            <ContentPresenter Content="{TemplateBinding ContentControl.Content}" ContentTemplate="{TemplateBinding ContentControl.ContentTemplate}" />
        </Border>
    </ControlTemplate>

</ResourceDictionary>
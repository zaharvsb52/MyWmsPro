    <SolidColorBrush x:Key="ToolTip.Foreground" Color="{StaticResource DefaultForegroundColor}" />
    
    <!-- GridSplitter -->
    <SolidColorBrush x:Key="GridSplitter.Background" Color="#FF80838F" />
    <SolidColorBrush x:Key="GridSplitter.PreviewBrush" Color="#FF201F35" />


            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#FFFFFFFF" Offset="0" />
              <GradientStop Color="#FFF1F2F6" Offset="1" />
            </LinearGradientBrush>
          </Border.Background>
          <Border.BorderBrush>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
              <GradientStop Color="#3FB8BAC2" Offset="0" />
              <GradientStop Color="#FFB8BAC2" Offset="1" />
            </LinearGradientBrush>
          </Border.BorderBrush>
        </Border>
        <StackPanel Orientation="Horizontal" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\GrayOne\Customization.xaml;49;18">
          <Image Margin="5,4,0,4" VerticalAlignment="Center" Source="{Binding Path=Glyph}" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\GrayOne\Customization.xaml;50;22" />
          <!--BEGIN WPF-->
          <TextBlock Foreground="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" Text="{Binding Path=Content}" Margin="5,4,10,4" HorizontalAlignment="Left" VerticalAlignment="Top" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\GrayOne\Customization.xaml;52;22" />
          <!--END WPF-->
          <!--ONLY SL
                    <TextBlock Text="{Binding Path=Content}" Margin="5,4,10,4"
                               HorizontalAlignment="Left" VerticalAlignment="Top"/>
                    ONLY SL-->
        </StackPanel>
      </Grid>
      <dxcore_unique_xmlns:DragIconControl x:Name="PART_DragIcon" DragType="{Binding Path=(dxb:BarDragProvider.DragType), RelativeSource={RelativeSource TemplatedParent}}" Margin="-12,-12,0,0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="1" dxcore_unique_xmlns:BlendHelper2.ThemeInfo="DevExpress.Xpf.Core\DevExpress.Xpf.Core\Bars\Themes\GrayOne\Customization.xaml;60;14" />
    </Grid>
  </ControlTemplate>

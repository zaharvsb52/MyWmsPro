                                        </Border>
                                    </Grid>
                                    <Grid x:Name="RightSelectedState" Margin="{StaticResource TabControlItem.SelectedState.Right.Root.Margin}" Opacity="0">
                                        <Border Background="{StaticResource TabControlItem.SelectedState.Border.Right}" Margin="{StaticResource TabControlItem.SelectedState.Right.Item.Margin}" Padding="{StaticResource TabControlItem.SelectedState.Right.Item.Padding}">
                                            <Border Background="{StaticResource TabControlItem.SelectedState.HighlightedBorder.Right}" Padding="{StaticResource TabControlItem.SelectedState.Right.Item.Padding}">
                                                <Border Background="{StaticResource TabControlItem.SelectedState.Background.Right}" />
                                            </Border>
                                        </Border>
                                        <Border x:Name="RightFocusedState" Background="{StaticResource TabControlItem.FocusedState.Right}" BorderThickness="0,2,2,2" Opacity="0" />
                                    </Grid>
                                </Grid>
                            </Grid>

                        </Grid>

                        <Grid x:Name="gridContainer">
                            <Grid x:Name="headerContainer" Margin="{TemplateBinding Padding}" MinHeight="{TemplateBinding MinHeight}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <dx:HeaderIconPresenter x:Name="headerIconPresenter" Content="{TemplateBinding Icon}" IconWidth="{TemplateBinding IconWidth}" IconHeight="{TemplateBinding IconHeight}" Margin="0,0,4,0" VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="0" IsTabStop="False" Visibility="Collapsed" />
                                <!--BEGIN WPF-->
                                <ContentPresenter Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Grid.Column="2">
                                    <ContentPresenter.RecognizesAccessKey>True</ContentPresenter.RecognizesAccessKey>
                                </ContentPresenter>
                                <!--END WPF-->
                                <!--ONLY SL<dx:DXContentPresenter Grid.Column="2" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>ONLY SL-->
                                <Button x:Name="closeButton" Margin="7,1,-4,0" HorizontalAlignment="Right" VerticalAlignment="Center" Style="{DynamicResource {dxt:DXTabItemThemeKey ResourceKey=CloseButtonStyle}}" Grid.Column="4" IsTabStop="False" Visibility="Collapsed" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </dx:HeaderContainerPresenter>
        </Grid>
    </ControlTemplate>

    <Thickness x:Key="{dxt:DXTabItemThemeKey ResourceKey=Padding}">9,3,9,3</Thickness>
    
</ResourceDictionary>
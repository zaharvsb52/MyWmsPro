                            <DoubleAnimation Storyboard.TargetName="TopFocusedState" Storyboard.TargetProperty="Opacity" Duration="0" To="1" />
                            <DoubleAnimation Storyboard.TargetName="BottomFocusedState" Storyboard.TargetProperty="Opacity" Duration="0" To="1" />

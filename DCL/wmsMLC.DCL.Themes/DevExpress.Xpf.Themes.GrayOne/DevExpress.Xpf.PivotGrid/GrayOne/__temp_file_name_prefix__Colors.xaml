    <wpf:GradientStop Color="#00C2C4CB" Offset="1" />
    <wpf:GradientStop Color="#FFC2C4CB" Offset="0.5" />
  </wpf:LinearGradientBrush>
  <wpf:Style x:Key="{dxpgi:AppearanceThemeKey ResourceKey=EmptyAreaHighlighterStyle}" TargetType="{x:Type Border}">
    <wpf:Setter Property="Background" Value="#FFE2EAFD" />
    <wpf:Setter Property="Margin" Value="-3" />
  </wpf:Style>
  <wpf:Style x:Key="{dxpgi:AppearanceThemeKey ResourceKey=FieldListDragOverBorderStyle}" TargetType="{x:Type Border}">
    <wpf:Setter Property="BorderBrush">
      <wpf:Setter.Value>
        <wpf:LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
          <wpf:GradientStop Color="#FF7E8FBB" Offset="0" />
          <wpf:GradientStop Color="#FFAEBDE6" Offset="1" />
        </wpf:LinearGradientBrush>
      </wpf:Setter.Value>
    </wpf:Setter>
    <wpf:Setter Property="Opacity" Value="0.9" />
  </wpf:Style>
